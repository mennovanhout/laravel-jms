{
    "name": "mennovanhout/laravel-jms",
    "description": "Adds jms serializer to Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "JMS",
        "Serializer"
    ],
    "version": "0.3.0",
    "require": {
        "jms/serializer": "^3.18.2",
        "doctrine/cache": "^2.2.0"
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Menno van Hout",
            "email": "info@mennovanhout.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "MennoVanHout\\JMS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MennoVanHout\\JMS\\JMSServiceProvider"
            ],
            "aliases": {
                "Serializer": "MennoVanHout\\JMS\\Facade"
            }
        }
    }
}
